record(ao, "$(DEVICE):DMOD_ROT_PHASE$(CHN)")
{
    field(DESC, "Demod Set Rot Phase")
    field(DTYP, "asynFloat64")
    field(OUT,  "@asyn($(PORT), 0) set_rot_phase_$(CHN)")
    field(OMSL, "supervisory")
    field(SCAN, "Passive")
    field(VAL,  "0.")
    field(PINI, "YES")
    info(autosaveFields, "VAL")
}

