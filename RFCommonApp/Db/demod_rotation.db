record(ai, "$(DEVICE):DMOD_PHASE$(CHN)")
{
    field(DESC, "Demod Phase")
    field(DTYP, "asynFloat64")
    field(INP,  "@asyn($(PORT), 0) phase_$(CHN)")
    field(PREC, "4")
    field(SCAN, "I/O Intr")
}

record(ai, "$(DEVICE):DMOD_AMPL$(CHN)")
